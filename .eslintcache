"messages":"87","errorCount":0,"fatalErrorCount":0,"warningCount":0,"fixableErrorCount":0,"fixableWarningCount":0},{"filePath":"88","messages":"89","errorCount":0,"fatalErrorCount":0,"warningCount":0,"fixableErrorCount":0,"fixableWarningCount":0},"/home/groot/dev/react_exercises/quotes-app/src/components/quotes/QuoteList.js",[],["90","91"],"/home/groot/dev/react_exercises/quotes-app/src/components/quotes/QuoteItem.js",[],"/home/groot/dev/react_exercises/quotes-app/src/components/UI/LoadingSpinner.js",[],"/home/groot/dev/react_exercises/quotes-app/src/pages/AllQuotes.js",[],"/home/groot/dev/react_exercises/quotes-app/src/pages/NotFound.js",[],"/home/groot/dev/react_exercises/quotes-app/src/hooks/use-http.js",[],"/home/groot/dev/react_exercises/quotes-app/src/lib/api.js",[],"/home/groot/dev/react_exercises/quotes-app/src/components/quotes/NoQuotesFound.js",[],"/home/groot/dev/react_exercises/quotes-app/src/index.tsx",[],["92","93"],"/home/groot/dev/react_exercises/quotes-app/src/App.tsx",["94"],"import { Route, Navigate, Routes } from \"react-router-dom\";\nimport { useState } from \"react\";\nimport AddQuote from \"./pages/AddQuote\";\nimport AllQuotes from \"./pages/AllQuotes\";\nimport QuoteDetail from \"./pages/QuoteDetail\";\nimport Layout from \"./components/layout/Layout\";\nimport NotFound from \"./pages/NotFound\";\n\nfunction App() {\n\n\n  return (\n    <Layout>\n      {/* in v5 we use Swtich, and in v6 react-router-dom we use Routes */}\n      <Routes>\n        {/* with v6 we don't need the \"exact\" tag anymore cuz it's always exact path */}\n        <Route path=\"/\" /* exact */ element={<Navigate replace to=\"quotes\" />} />\n        {/* in v5 we used Redirect in v6 we use \"Navigate replace to\" within the Route */}\n        {/* <Redirect to=\"/quotes\" /> */}\n\n        {/* in v5 we wrapped the Route component around the shown component\n        in v6 we have a self-closing Route component with element that contains the shown component */}\n        <Route path=\"/quotes\" element={<AllQuotes />} />\n        <Route\n        /* To allow nested routes within this route use \"*\" */\n          path=\"/quotes/:quoteId/*\"\n          element={<QuoteDetail />}\n        />\n        <Route\n          path=\"/new-quote\"\n          element={<AddQuote />}\n        />\n        {/* To add a catch-all/404/not found page just put the path to the star and at the total end of the Switch */}\n        <Route path=\"*\" element={<NotFound />} />\n      </Routes>\n    </Layout>\n  );\n}\n\nexport default App;\n","/home/groot/dev/react_exercises/quotes-app/src/pages/AddQuote.tsx",[],"/home/groot/dev/react_exercises/quotes-app/src/components/quotes/QuoteForm.tsx",["95"],"import React, { useRef, useState } from \"react\";\n\nimport Card from \"../UI/Card\";\nimport Quote from \"../../models/quote\";\nimport LoadingSpinner from \"../UI/LoadingSpinner\";\nimport classes from \"./QuoteForm.module.css\";\n\ntype Props = {\n  onAddQuote: (quote: {author: Quote[\"author\"], text: Quote[\"text\"]}) => void;\n  isLoading: boolean;\n};\n\nconst QuoteForm: React.FC <Props> = (props) => {\n  const authorInputRef = useRef<HTMLTextAreaElement & HTMLInputElement>(null);\n  const textInputRef = useRef<HTMLTextAreaElement & HTMLInputElement>(null);\n  const [isEntering, setIsEntering] = useState(false);\n\n  function submitFormHandler(event: React.FormEvent) {\n    event.preventDefault();\n\n    const enteredAuthor = authorInputRef.current!.value;\n    const enteredText = textInputRef.current!.value;\n\n    // optional: Could validate here\n\n    props.onAddQuote({ author: enteredAuthor, text: enteredText });\n  }\n\n  const handleFormFocus = () => {\n    //when the user clicked into any form field, we set it true\n    //this is then used to show the Prompt when leaving the page\n    setIsEntering(true);\n  };\n\n  return (\n    <React.Fragment>\n      {/* to show a prompt warning if a user typed in something into the form and clicks on the back button/leaves the page\n      we can use the Prompt component from react-router-dom */}\n      {/*  in v6 the Prompt component is no longer supported\n      <Prompt\n        when={isEntering}\n        /* with location we could also show the path \n        message={(location) =>\n          \"Are you sure you want to leave? All your entered data will be lost.\"\n        }\n      /> */}\n\n      <Card>\n        <form\n          onFocus={handleFormFocus}\n          className={classes.form}\n          onSubmit={submitFormHandler}\n        >\n          {props.isLoading && (\n            <div className={classes.loading}>\n              <LoadingSpinner />\n            </div>\n          )}\n\n          <div className={classes.control}>\n            <label htmlFor=\"author\">Author</label>\n            <input type=\"text\" id=\"author\" ref={authorInputRef} />\n          </div>\n          <div className={classes.control}>\n            <label htmlFor=\"text\">Text</label>\n            <textarea id=\"text\" rows= {5} ref={textInputRef}></textarea>\n          </div>\n          <div className={classes.actions}>\n            {/* to not get the prompt when we actually wanna submit the quote we set it to false */}\n            <button onClick={() => setIsEntering(false)} className=\"btn\">\n              Add Quote\n            </button>\n          </div>\n        </form>\n      </Card>\n    </React.Fragment>\n  );\n};\n\nexport default QuoteForm;\n","/home/groot/dev/react_exercises/quotes-app/src/components/UI/Card.tsx",[],"/home/groot/dev/react_exercises/quotes-app/src/components/layout/Layout.tsx",[],"/home/groot/dev/react_exercises/quotes-app/src/components/layout/MainNavigation.tsx",[],"/home/groot/dev/react_exercises/quotes-app/src/pages/QuoteDetail.tsx",[],"/home/groot/dev/react_exercises/quotes-app/src/components/comments/Comments.tsx",[],"/home/groot/dev/react_exercises/quotes-app/src/components/comments/CommentsList.tsx",[],"/home/groot/dev/react_exercises/quotes-app/src/compone